{
  "name": "lumen-cms-nextjs",
  "version": "1.1.9",
  "description": "NextJS and Storyblok with LumenCMS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "rimraf app-content*json && next",
    "analyze": "BUNDLE_ANALYZE=true npm run build",
    "start": "NODE_ENV=production && next start",
    "build": "rimraf app-content*json &&  next build",
    "build-ts": "tsdx build --tsconfig ./tsconfig.build.json --target node",
    "watch": "tsdx watch --tsconfig ./tsconfig.build.json --target node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build-ts"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Dominic Garms",
  "license": "MIT",
  "dependencies": {
    "next-compose-plugins": "2.2.0",
    "next-transpile-modules": "3.3.0",
    "sitemap": "6.1.5"
  },
  "peerDependencies": {
    "next": "9.4.4",
    "lumen-cms-core": "^0.41.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "next": "9.4.4",
    "lumen-cms-core": "^0.41.1",
    "@next/bundle-analyzer": "^9.4.4",
    "@types/node": "^14.0.5",
    "tsdx": "^0.13.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
